{
	"name": "kakeru-baby-membership",
	"version": "1.0.0",
	"description": "",
	"main": "server.js",
	"scripts": {
		"build": "rimraf -rf server.js app.js ./backend ./schemas ./contracts && tsc",
		"dev": "nodemon -r dotenv/config ./backend-src/server.ts",
		"start": "node server.js",
		"pretest": "ts-node ./backend-src/backend/devOps/db/index.ts -- init:force,manager",
		"test": "jest --coverage --runInBand",
		"lint": "eslint --cache \"backend-src/**/*.{ts,d.ts}\"",
		"lint:fix": "eslint --fix --ext \"backend-src/**/*.{ts,d.ts}\"",
		"deptestServer": "depcruise --exclude \"^node_modules\" --output-type dot ./backend-src/server.ts | dot -T svg -o dependency_graphs/dependencyGraph.svg",
		"quickSync": "node ./backend/devOps/db/index.js --trace-warnings -- init:sync",
		"initManager": "node ./backend/devOps/db/index.js --trace-warnings -- init:manager",
		"db:migrate": "ts-node ./backend-src/backend/devOps/db/index.ts",
		"db:init": "node ./backend/devOps/db/index.js -- init:sync,manager",
		"clinic:doctor": "clinic doctor --on-port 'autocannon -c 10 -d 10 -m GET localhost:$PORT/api/m/categories' -- node server.js",
		"clinic:bubbleprof": "clinic bubbleprof --on-port 'autocannon -c 5 -a 500 -m GET localhost:$PORT/api/m/categories' -- node server.js",
		"pretty": "prettier --write ./backend-src",
		"prepare": "husky install"
	},
	"engines": {
		"node": ">=18.20.3"
	},
	"author": "Miraic",
	"license": "UNLICENSED",
	"private": "true",
	"dependencies": {
		"@line/bot-sdk": "^7.5.2",
		"@ts-rest/core": "^3.49.2",
		"@ts-rest/express": "^3.49.2",
		"ajv": "^8.11.0",
		"axios": "^1.7.4",
		"axios-retry": "^4.5.0",
		"bcryptjs": "^2.4.3",
		"connect-session-sequelize": "^7.1.5",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.18.2",
		"express-session": "^1.17.2",
		"generate-password": "^1.7.0",
		"helmet": "^7.0.0",
		"json2csv": "^6.0.0-alpha.2",
		"jsonwebtoken": "^9.0.0",
		"mime-types": "^2.1.35",
		"moment": "^2.29.4",
		"multer": "^1.4.5-lts.1",
		"mysql2": "^3.3.1",
		"qrcode": "^1.5.3",
		"sequelize": "^6.28.0",
		"sharp": "^0.33.4",
		"socket.io": "^4.5.4",
		"winston": "^3.8.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/cookie-parser": "^1.4.7",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.15",
		"@types/express-session": "^1.17.5",
		"@types/jest": "^29.5.12",
		"@types/json2csv": "^5.0.7",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/mime-types": "^2.1.4",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.19.45",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"@typescript-eslint/parser": "^5.47.0",
		"dependency-cruiser": "^12.12.1",
		"eslint": "^8.30.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-security": "^1.5.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"prettier": "^2.8.1",
		"sequelize-cli": "^6.6.0",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.4"
	}
}
