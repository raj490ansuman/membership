	"type": "module",
	"scripts": {
		"dev": "vite --mode ci",
		"preview": "vite preview --port 3000",
		"build:staging": "tsc --noEmit & vite build --minify --mode staging",
		"build:production": "tsc --noEmit & vite build --minify --mode production",
		"build:css": "postcss src/assets/tailwind.css -o src/assets/tailwind-generated.css",
		"build:css:prod": "postcss src/assets/tailwind.css -o src/assets/tailwind-generated.css --env production --minify",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint --fix",
		"format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config",
		"analyze": "source-map-explorer 'build/static/js/*.js'"
	},
	"engines": {
		"node": ">=18.20.4"
	},
